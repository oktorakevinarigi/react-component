  "tsup": {
    "clean": true,
    "dts": true,
    "minify": true,
    "target": "es2019",
    "format": [
      "cjs",
      "esm"
    ],
    "external": [
      "react",
      "react-dom"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {},
  "dependencies": {
    "@chakra-ui/icons": "^2.0.17",
    "@chakra-ui/react": "^2.5.1",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "framer-motion": "^10.1.0"
